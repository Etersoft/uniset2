	<UserData/>
	<!-- Общие(стартовые) параметры по UniSet -->
	<UniSet>
		<NameService host="localhost" port="2809"/>
		<LocalNode name="localhost"/>
		<RootSection name="UNISET_SM"/>
		<CountOfNet name="1"/>
		<RepeatCount name="3"/>
		<RepeatTimeoutMS name="50"/>
		<WatchDogTime name="0"/>
		<PingNodeTime name="0"/>
		<AutoStartUpTime name="1"/>
